{
  "name": "canai-platform",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "vitest run --coverage",
    "test:unit": "vitest run tests/unit/ --coverage",
    "test:integration": "vitest run tests/integration/ --coverage",
    "typecheck": "tsc --noEmit",
    "migrate:test": "npx supabase db migrate --env test",
    "test:supabase:connection": "vitest run tests/integration/supabase/*.test.ts tests/integration/supabase/*.integration.test.ts",
    "check:supabase:connection": "node scripts/check-supabase.js",
    "test:ci": "vitest run --reporter=dot --reporter=junit --outputFile=./test-results.xml --silent"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "turbo": "^1.10.0",
    "typescript": "^5.3.0",
    "vite": "7.0.0"
  },
  "dependencies": {
    "@ai-sdk/hume": "^1.0.0-canary.11",
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@memberstack/react": "^2.0.0",
    "@sentry/node": "^7.0.0",
    "@sentry/react": "^7.0.0",
    "@stripe/stripe-js": "^2.0.0",
    "@supabase/auth-helpers-react": "^0.0.0",
    "@supabase/supabase-js": "2.50.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    "@tanstack/react-query": "^5.0.0",
    "autoprefixer": "^10.0.0",
    "clsx": "^2.0.0",
    "cors": "2.8.5",
    "date-fns": "^2.0.0",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "helmet": "8.1.0",
    "hume": "^0.11.4",
    "ioredis": "^5.0.0",
    "joi": "17.13.3",
    "morgan": "1.10.0",
    "postcss": "^8.0.0",
    "posthog-js": "^1.0.0",
    "rate-limiter-flexible": "7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.0",
    "redis": "^4.0.0",
    "stripe": "^14.0.0",
    "tailwindcss": "^3.0.0",
    "uuid": "9.0.1",
    "zod": "^3.0.0",
    "zustand": "^4.0.0"
  },
  "engines": {
    "node": ">=18.19.0"
  },
  "type": "module"
}
