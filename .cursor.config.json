  "taskmaster": {
    "enabled": true,
    "paths": {
      "prd": ".taskmaster/docs/prd.txt",
      "tasks": ".taskmaster/tasks/tasks.json",
      "cortexMilestones": ".cursor/rules/cortex.md",
      "cortexRules": ".cursor/rules/canai-cortex-rules.mdc"
    },
    "aiModel": "claude-3-7-sonnet-20250219",
    "fieldNaming": {
      "convention": "kebab-case",
      "cursorInstructionsField": "cursor-ai-instructions"
    },
    "validationRules": {
      "enforceTaskStructure": true,
      "validateDependencies": true,
      "requirePRDAlignment": true,
      "mandatoryFields": ["id", "description", "inputs", "outputs", "dependencies", "cursor-ai-instructions"],
      "optionalFields": ["status", "priority", "complexity", "estimated_hours"]
    },
    "yamlToJsonConversion": {
      "preserveKebabCase": true,
      "convertToSnakeCase": false
    },
    "integration": {
      "cortexMilestones": true,
      "prdSectionMapping": true,
      "dependencyValidation": true,
      "cursorRulesSync": true
    }
  }
}
